{
    "name": "yiisoft/profiler",
    "type": "library",
    "description": "Profiler",
    "keywords": [
        "profiler",
        "performance"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/profiler/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/profiler"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "psr/log": "^1.1",
        "yiisoft/files": "^3.0@dev",
        "yiisoft/strings": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.6",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Profiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Profiler\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "common": "config/common.php",
            "events-console": "config/events-console.php",
            "events-web": "config/events-web.php",
            "params": "config/params.php"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
