{
    "name": "yiisoft/profiler",
    "type": "library",
    "description": "Profiler",
    "keywords": [
        "profiler",
        "performance"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/profiler/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/profiler"
    },
    "require": {
        "php": "^7.4|^8.0",
        "psr/log": "^1.1",
        "yiisoft/files": "^1.0",
        "yiisoft/strings": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.14",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.18"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Profiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Profiler\\Tests\\": "tests"
        }
    },
    "extra": {
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "common": "common.php",
            "events-web": "events-web.php",
            "params": "params.php"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
